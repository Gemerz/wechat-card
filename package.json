{
  "name": "Wechat-card",
  "version": "1.0.0",
  "description": "Wechat card preivew",
  "private": true,
  "author": "Gemer Cheung",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run dev",
    "dev": "node ./node_modules/.bin/better-npm-run dev",
		"compile": "node ./node_modules/.bin/better-npm-run compile",
    "dev:no-debug": "npm run dev -- --no_debug"
  },
  "betterScripts": {
    "dev": {
      "command": "node --harmony bin/webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:dev": {
      "command": "npm run test -- --watch",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.2.0",
    "better-npm-run": "0.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.3",
    "less": "^2.6.0",
    "less-loader": "^4.0.5",
    "node-sass": "^4.7.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.4",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "antd": "^2.13.10",
    "bootstrap": "^3.3.5",
    "classnames": "^2.2.0",
    "history": "^4.7.2",
    "immutable": "^3.7.5",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.0.0",
    "redux-2way-binding": "0.0.8",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.0.1",
    "yargs": "^10.0.3"
  }
}
